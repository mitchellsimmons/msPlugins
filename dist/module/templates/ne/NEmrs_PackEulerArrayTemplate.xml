<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEmrs_PackEulerArray'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.mrs_EulerArray'>
			<label>Output</label>
		</attribute>
		<attribute name='input' type='maya.double3'>
			<label>Input</label>
		</attribute>
		<attribute name='fill' type='maya.double3'>
			<label>Fill</label>
		</attribute>
    <attribute name='fillLogically' type='maya.boolean'>
      <label>Fill Logically</label>
    </attribute>
		<attribute name='size' type='maya.long'>
			<label>Size</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEmrs_PackEulerArray'>
		<property name='message'/>
		<property name='output'/>
		<property name='input'/>
    <property name='fill'/>
    <property name='fillLogically'/>
		<property name='size'/>
	</view>
</templates>
