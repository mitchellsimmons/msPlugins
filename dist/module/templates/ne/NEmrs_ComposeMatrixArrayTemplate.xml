<?xml version='1.0' encoding='UTF-8'?>
<templates>
  <using package='maya'/>
  <using package='std'/>
  <template name='NEmrs_ComposeMatrixArray'>
    <attribute name='message' type='maya.message'>
      <label>Message</label>
    </attribute>
    <attribute name='output' type='maya.matrixArray'>
      <label>Output</label>
    </attribute>
    <attribute name='translation' type='maya.vectorArray'>
      <label>Translation</label>
    </attribute>
    <attribute name='rotation' type='maya.mrs_EulerArray'>
      <label>Rotation</label>
    </attribute>
    <attribute name='scale' type='maya.vectorArray'>
      <label>Scale</label>
    </attribute>
    <attribute name='quaternion' type='maya.mrs_QuaternionArray'>
      <label>Quaternion</label>
    </attribute>
    <attribute name='rotationOrder' type='maya.Int32Array'>
      <label>Rotation Order</label>
    </attribute>
    <attribute name='useEulerRotation' type='maya.boolean'>
      <label>Use Euler Rotation</label>
    </attribute>
    <attribute name='size' type='maya.long'>
      <label>Size</label>
    </attribute>
  </template>
  <view name='NEDefault' template='NEmrs_ComposeMatrixArray'>
    <property name='message'/>
    <property name='output'/>
    <property name='translation'/>
    <property name='rotation'/>
    <property name='scale'/>
    <property name='quaternion'/>
    <property name='rotationOrder'/>
    <property name='useEulerRotation'/>
    <property name='size'/>
  </view>
</templates>
