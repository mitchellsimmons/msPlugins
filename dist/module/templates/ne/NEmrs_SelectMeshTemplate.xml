<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEmrs_SelectMesh'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.mesh'>
			<label>Output</label>
		</attribute>
		<attribute name='condition' type='maya.bool'>
			<label>Condition</label>
		</attribute>
		<attribute name='ifFalse' type='maya.mesh'>
			<label>If False</label>
		</attribute>
		<attribute name='ifTrue' type='maya.mesh'>
			<label>If True</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEmrs_SelectMesh'>
		<property name='message'/>
		<property name='output'/>
		<property name='condition'/>
		<property name='ifFalse'/>
		<property name='ifTrue'/>
	</view>
</templates>
