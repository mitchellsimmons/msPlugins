<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEmrs_PolarToCartesian'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='outputX' type='maya.double'>
			<label>Output X</label>
		</attribute>
    <attribute name='outputY' type='maya.double'>
      <label>Output Y</label>
    </attribute>
		<attribute name='angle' type='maya.doubleAngle'>
			<label>Angle</label>
		</attribute>
    <attribute name='radius' type='maya.double'>
      <label>Radius</label>
    </attribute>
	</template>
	<view name='NEDefault' template='NEmrs_PolarToCartesian'>
		<property name='message'/>
    <property name='outputX'/>
    <property name='outputY'/>
    <property name='angle'/>
    <property name='radius'/>
	</view>
</templates>
