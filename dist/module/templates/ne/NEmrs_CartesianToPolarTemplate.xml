<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEmrs_CartesianToPolar'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='outputAngle' type='maya.doubleAngle'>
			<label>Output Angle</label>
		</attribute>
    <attribute name='outputRadius' type='maya.double'>
      <label>Output Radius</label>
    </attribute>
		<attribute name='x' type='maya.double'>
			<label>X</label>
		</attribute>
    <attribute name='y' type='maya.double'>
      <label>Y</label>
    </attribute>
	</template>
	<view name='NEDefault' template='NEmrs_CartesianToPolar'>
		<property name='message'/>
    <property name='outputAngle'/>
    <property name='outputRadius'/>
    <property name='x'/>
    <property name='y'/>
	</view>
</templates>
