<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}AimTransform'>
    <attribute name='message' type='maya.message'>
      <label>Message</label>
    </attribute>
    <attribute name='inverseMatrix' type='maya.matrix'>
      <label>Inverse Matrix</label>
    </attribute>
    <attribute name='matrix' type='maya.matrix'>
      <label>Matrix</label>
    </attribute>
    <attribute name='parentInverseMatrix' type='maya.matrix'>
      <label>Parent Inverse Matrix</label>
    </attribute>
    <attribute name='parentMatrix' type='maya.matrix'>
      <label>Parent Matrix</label>
    </attribute>
    <attribute name='worldInverseMatrix' type='maya.matrix'>
      <label>World Inverse Matrix</label>
    </attribute>
    <attribute name='worldMatrix' type='maya.matrix'>
      <label>World Matrix</label>
    </attribute>
		<attribute name='translate' type='maya.double3'>
			<label>Translate</label>
		</attribute>
		<attribute name='rotate' type='maya.double3'>
			<label>Rotate</label>
		</attribute>
		<attribute name='scale' type='maya.double3'>
			<label>Scale</label>
		</attribute>
		<attribute name='aimAxis' type='maya.enum'>
			<label>Aim Axis</label>
		</attribute>
		<attribute name='upAxis' type='maya.enum'>
			<label>Up Axis</label>
		</attribute>
		<attribute name='rootPosition' type='maya.matrix'>
			<label>Root Position</label>
		</attribute>
		<attribute name='aimPosition' type='maya.matrix'>
			<label>Aim Position</label>
		</attribute>
		<attribute name='upPosition' type='maya.matrix'>
			<label>Up Position</label>
		</attribute>
		<attribute name='parentInverseFrame' type='maya.matrix'>
			<label>Parent Inverse Frame</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}AimTransform'>
    <property name='message'/>
    <property name='inverseMatrix'/>
    <property name='matrix'/>
    <property name='parentInverseMatrix'/>
    <property name='parentMatrix'/>
    <property name='worldInverseMatrix'/>
    <property name='worldMatrix'/>
    <property name='translate'/>
    <property name='rotate'/>
    <property name='scale'/>
    <property name='aimAxis'/>
    <property name='upAxis'/>
    <property name='rootPosition'/>
    <property name='aimPosition'/>
    <property name='upPosition'/>
    <property name='parentInverseFrame'/>
	</view>
</templates>