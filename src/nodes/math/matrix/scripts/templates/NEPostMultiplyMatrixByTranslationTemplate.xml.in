<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}PostMultiplyMatrixByTranslation'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.matrix'>
			<label>Output</label>
		</attribute>
		<attribute name='input' type='maya.matrix'>
			<label>Input</label>
		</attribute>
		<attribute name='translation' type='maya.double3'>
			<label>Translation</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}PostMultiplyMatrixByTranslation'>
		<property name='message'/>
		<property name='output'/>
		<property name='input'/>
		<property name='translation'/>
	</view>
</templates>