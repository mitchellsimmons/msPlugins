<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}ComposeMatrix'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.matrix'>
			<label>Output</label>
		</attribute>
		<attribute name='translation' type='maya.double3'>
			<label>Translation</label>
		</attribute>
		<attribute name='rotation' type='maya.double3'>
			<label>Rotation</label>
		</attribute>
		<attribute name='scale' type='maya.double3'>
			<label>Scale</label>
		</attribute>
		<attribute name='quaternion' type='maya.TdataCompound'>
			<label>Quaternion</label>
		</attribute>
		<attribute name='rotationOrder' type='maya.enum'>
			<label>Rotation Order</label>
		</attribute>
		<attribute name='useEulerRotation' type='maya.bool'>
			<label>Use Euler Rotation</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}ComposeMatrix'>
		<property name='message'/>
		<property name='output'/>
		<property name='translation'/>
		<property name='rotation'/>
		<property name='scale'/>
		<property name='quaternion'/>
		<property name='rotationOrder'/>
		<property name='useEulerRotation'/>
	</view>
</templates>