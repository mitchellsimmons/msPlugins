<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}WeightedAverageEuler'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.double3'>
			<label>Output</label>
		</attribute>
		<attribute name='input' type='maya.${NODE_NAME_PREFIX}EulerArray'>
			<label>Input</label>
		</attribute>
		<attribute name='weight' type='maya.doubleArray'>
			<label>Weight</label>
		</attribute>
		<attribute name='inputRotationOrder' type='maya.Int32Array'>
			<label>Input Rotation Order</label>
		</attribute>
		<attribute name='outputRotationOrder' type='maya.enum'>
			<label>Output Rotation Order</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}WeightedAverageEuler'>
		<property name='message'/>
		<property name='output'/>
		<property name='input'/>
		<property name='weight'/>
		<property name='inputRotationOrder'/>
		<property name='outputRotationOrder'/>
	</view>
</templates>