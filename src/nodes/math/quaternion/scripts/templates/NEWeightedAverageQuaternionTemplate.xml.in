<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}WeightedAverageQuaternion'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.TdataCompound'>
			<label>Output</label>
		</attribute>
		<attribute name='input' type='maya.${NODE_NAME_PREFIX}QuaternionArray'>
			<label>Input</label>
		</attribute>
		<attribute name='weight' type='maya.doubleArray'>
			<label>Weight</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}WeightedAverageQuaternion'>
		<property name='message'/>
		<property name='output'/>
		<property name='input'/>
		<property name='weight'/>
	</view>
</templates>