<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NE${NODE_NAME_PREFIX}Power'>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='output' type='maya.double'>
			<label>Output</label>
		</attribute>
		<attribute name='base' type='maya.double'>
			<label>Base</label>
		</attribute>
		<attribute name='exponent' type='maya.double'>
			<label>Exponent</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NE${NODE_NAME_PREFIX}Power'>
		<property name='message'/>
		<property name='output'/>
		<property name='base'/>
		<property name='exponent'/>
	</view>
</templates>